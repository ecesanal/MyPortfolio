@model Skill
@{
    ViewData["Title"] = "UpdateSkill";
    Layout = "~/Views/Layout/Index.cshtml";
}
<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Yetenek Bilgileri</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Yetenek Bilgileri Güncelleme Sayfası</div>
					</div>
					<div class="card-body">
						<form method="post">
							<input type="hidden" asp-for="SkillId" />
							<label>Yetenek Başlığı</label>
							<input type="text" asp-for="Title" class="form-control" />
							<br />
							<label>Yetenek İçeriği</label>
							<input type="text" asp-for="Value" class="form-control" />
							<br />
							<button class="btn btn-warning">Yetenek Bilgilerini Güncelle</button>
						</form>
					</div>
					<br />
					<br />
					<br />
					<br />
					<br />
					<br />
					<br />
					<br />
					<br />
					<br />
					<br />
				</div>
			</div>
		</div>
	</div>
</div>

